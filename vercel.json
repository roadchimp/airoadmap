{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/server/index.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/run-job-scrapers",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/process-job-descriptions",
      "schedule": "0 2 * * *"
    }
  ]
} 